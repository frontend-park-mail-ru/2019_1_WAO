<form class="profile-form">
  {{> ../title/title.hbs title}}
  <div class="profile-card">
    <div class="profile-card__fileds">
      <input class="profile-input" type="text" value={{ nickname }} name="nickname" readonly>
      <input class="profile-input" type="text" value={{ email }} name="email">
      <input class="profile-input" type="password" value="" name="password" placeholder="Введите новый пароль">
    </div>
    <div class="profile-card__button">
    {{> ../form-button/form-button.hbs button}}
    </div>
    <div>
      <img class="profile-card__image" src={{ image }}>
      <input class="profile-form__image-input" type="file">
    </div>
    <div class="profile-card__labels">
      <a class="profile-card__label" data-href="#">Счет: {{ score }}</a>
      <a class="profile-card__label" data-href="#">Побед: {{ wins }}/{{ games }}</a>
    </div>
  </div>
</form>