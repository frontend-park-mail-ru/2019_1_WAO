<div class="userbar">
    <image class="userbar__image" src={{image}} data-href="/profile"></image>
    <a class="userbar__nickname" data-href="/profile">
        {{nickname}}
    </a>
    <a class="userbar__score" data-href="/users">
        {{score}}
    </a>
    <image class="userbar__menu" src="./images/menu.png" data-href="/"></image>
    <image class="userbar__door" src="./images/door.svg" data-href="#"></image>
</div>